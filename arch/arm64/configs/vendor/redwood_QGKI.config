# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_TEE=y
CONFIG_INPUT_FINGERPRINT=y


# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW86927_HAPTIC=y

# IO Schedulers
CONFIG_CFQ_GROUP_IOSCHED=y

# IR
CONFIG_IR_SPI=y
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Pinctrl
CONFIG_PINCTRL_SM7325=y
CONFIG_PINCTRL_REDWOOD=y

# Power Supply
CONFIG_BQ2597X=y
CONFIG_BQ_CLOUD_AUTHENTICATION=y
CONFIG_BQ_FG_1S=y
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_LEDS_QTI_FLASH=m

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916=y
CONFIG_TOUCHSCREEN_QGKI_GOODIX=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y
CONFIG_TOUCHSCREEN_FTS=y

# WiFi

CONFIG_CNSS2=m
CONFIG_CNSS_QCA6750=y
CONFIG_ICNSS2=m
CONFIG_QCA_CLD_WLAN=m

# File systems
CONFIG_EXFAT_FS=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=y

# RCU Subsystem
CONFIG_BLK_CGROUP=y

